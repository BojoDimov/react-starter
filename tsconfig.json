{
  "compilerOptions": {
    "lib": [
      "es6",
      "dom"
    ],
    "sourceMap": true,
    "jsx": "react",
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es6",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
  }
}